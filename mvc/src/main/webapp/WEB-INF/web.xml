    <filter-name>ESFORCO</filter-name>
    <url-pattern>/campeonato</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>AUTORIZACAO</filter-name>
    <filter-class>br.dev.mtparreira.campeonato.filter.Autorizacao</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AUTORIZACAO</filter-name>
    <url-pattern>/campeonato</url-pattern>
  </filter-mapping>
</web-app>